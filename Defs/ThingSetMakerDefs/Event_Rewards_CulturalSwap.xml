<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!-- Nothing to translate in here. -->

    <ThingSetMakerDef>        <!-- (gold?) statue. -->
        <defName>Reward_culturalSwapFirstPlace</defName>
        <root Class="ThingSetMaker_RandomOption">
            <options>
                <li>
                    <weight>6</weight>
                    <thingSetMaker Class="ThingSetMaker_StackCount">
                        <fixedParams>
                            <maxThingMarketValue>5000</maxThingMarketValue>
                            <filter>
                                <categories>
                                    <li>BuildingsArt</li>
                                </categories>
                            </filter>
                        </fixedParams>
                    </thingSetMaker>
                </li>
            </options>
        </root>
    </ThingSetMakerDef>

    <ThingSetMakerDef>        <!-- piles of silver/ healer serum -->
        <defName>Reward_culturalSwapFirstOther</defName>
        <root Class="ThingSetMaker_Sum">
            <fixedParams>
                <totalMarketValueRange>2500</totalMarketValueRange>
            </fixedParams>
            <options>
                <li>
                    <chance>0.8</chance>
                    <thingSetMaker Class="ThingSetMaker_StackCount">
                        <fixedParams>
                            <filter>
                                <thingDefs>
                                    <li>Silver</li>
                                </thingDefs>
                            </filter>
                            <countRange>
                                <min>400</min>
                                <max>1200</max>
                            </countRange>
                        </fixedParams>
                    </thingSetMaker>
                </li>
                <li>
                    <chance>0.7</chance>
                    <thingSetMaker Class="ThingSetMaker_StackCount">
                        <fixedParams>
                            <filter>
                                <thingDefs>
                                    <li>Silver</li>
                                </thingDefs>
                            </filter>
                            <countRange>
                                <min>400</min>
                                <max>1200</max>
                            </countRange>
                        </fixedParams>
                    </thingSetMaker>
                </li>
                <li>
                    <chance>0.3</chance>
                    <thingSetMaker Class="ThingSetMaker_Count">
                        <fixedParams>
                            <qualityGenerator>Reward</qualityGenerator>
                            <filter>
                                <thingDefs>
                                    <li>MechSerumHealer</li>
                                    <li>MechSerumResurrector</li>
                                </thingDefs>
                            </filter>
                        </fixedParams>
                    </thingSetMaker>
                </li>
            </options>
        </root>
    </ThingSetMakerDef>

    <ThingSetMakerDef>        <!-- the equivalent of Rotten tomatoes -->
        <defName>Reward_culturalSwapFirstLoser</defName>
        <root Class="ThingSetMaker_Sum">
            <options>
                <li>
                    <thingSetMaker Class="ThingSetMaker_Nutrition">
                        <fixedParams>
                            <filter>
                                <categories>
                                    <li>Foods</li>
                                </categories>
                                <disallowedThingDefs>
                                    <li>InsectJelly</li>
                                </disallowedThingDefs>
                                <disallowWorsePreferability>RawBad</disallowWorsePreferability>
                                <disallowWithComp>CompHatcher</disallowWithComp>
                                <disallowInedibleByHuman>true</disallowInedibleByHuman>
                            </filter>
                            <totalNutritionRange>
                                <min>4</min>
                                <max>8</max>
                            </totalNutritionRange>
                        </fixedParams>
                    </thingSetMaker>
                </li>
            </options>
        </root>
    </ThingSetMakerDef>

</Defs>